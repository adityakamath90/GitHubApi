<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/detailPaddingLeft"
    tools:context="com.githubapi.repositoryDetail.view.RepositoryDetailActivity">


    <TextView
        android:id="@+id/textViewIssueList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="@dimen/contributorTopMargin"
        android:layout_below="@+id/textViewIssueList"
        android:id="@+id/textViewContributorList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>
